{
    "manifest_version": 2,
    "name": "Vobla",
    "description": "Hides ‘Foreign agents’ disclaimers from Meduza's facebook posts and from articles on their website",
    "version": "0.1",
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	  },
    "content_scripts": [{
        "matches": ["https://*.facebook.com/*", "https://*.meduza.io/*"],
        "js": ["content.js"]
    }]
}